<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        
        <record id="hc_contact_action" model="ir.actions.act_window">
            <field name="name">Contact</field>
            <field name="res_model">hc.contact</field>
            <field name="view_mode">tree,form</field>
        </record>

        <!-- hc.contact tree view -->
        <record id="hc_contact_view_tree" model="ir.ui.view">
            <field name="name">hc.contact.view.tree</field>
            <field name="model">hc.contact</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="first_name"/>
                    <field name="last_name"/>
                    <field name="birth_day"/>
                    <field name="age"/>
                </tree>
            </field>
        </record>
        
    </data>
</odoo>