<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <!-- hc_supply action window -->
    <record id="hc_supply_action" model="ir.actions.act_window">
        <field name="name">Supply</field>
        <field name="res_model">hc.supply</field>
        <field name="view_mode">tree,form</field>
    </record>

    <!-- hc_supply tree view -->
    <record id="hc_supply_view_tree" model="ir.ui.view">
        <field name="name">hc.supply.view.tree</field>
        <field name="model">hc.supply</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <!-- hc_supply form view -->
    <record id="hc_supply_view_form" model="ir.ui.view">
        <field name="name">hc.supply.view.form</field>
        <field name="model">hc.supply</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="description"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>


</odoo>